# TeX distribution
set --export PATH /usr/texbin $PATH
# Homebrew python
set --export PATH /usr/local/share/python $PATH
# Homebrew python3
set --export PATH /usr/local/share/python3 $PATH
# Homebrew Ruby gems
set --export PATH /usr/local/opt/ruby/bin $PATH
# Homebrew
set --export PATH /usr/local/bin /usr/local/sbin $PATH
